FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      probesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Fields to be probed. runTime modifiable!
fields
(
    U
    T
);

// Locations to be probed. runTime modifiable!
probeLocations
(
    (0.0055 0 0.001)
    (0.0065 0 0.001)
    (0.0075 0 0.001)
);

